{
    "name": "sispon",
    "version": "1.7.5",
    "description": "Sistema de Pontos Empresarial",
    "main": "src/website/app.js",
    "scripts": {
        "prestart": "npm i --loglevel silent",
        "start": "node src/website/app.js",
        "build": "node ./build.js",
        "bot": "node src/telegram/bot.js"
    },
    "type": "commonjs",
    "keywords": [
        "pontos",
        "sistema de pontos",
        "ponto empresarial",
        "sistema de ponto empresarial"
    ],
    "author": "Tocka",
    "license": "ISC",
    "dependencies": {
        "@angablue/exe": "^1.2.0",
        "7zip-bin": "^5.2.0",
        "bcrypt": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-ws": "^5.0.2",
        "module-alias": "^2.2.2",
        "moment": "^2.29.4",
        "mongoose": "^6.8.2",
        "nanoid": "^3.3.0",
        "nfc-pcsc": "^0.8.1",
        "node-7z": "^3.0.0",
        "node-telegram-bot-api": "^0.61.0",
        "open": "^8.4.0",
        "pkg": "^5.8.0",
        "pkg-fetch": "^3.4.2",
        "resedit": "^1.6.1"
    },
    "bin": "src/website/app.js",
    "pkg": {
        "scripts": [
            "src/**/*.js"
        ],
        "assets": [
            "src/website/public/**/*",
            "node_modules/**/*"
        ],
        "targets": [
            "node16-win-x64",
            "node16-x86"
        ],
        "outputPath": "bin/"
    }
}
